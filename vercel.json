{
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x"
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "https://admanager.google.com",
      "permanent": true
    },
    {
      "source": "/((?!api).*)",
      "destination": "https://admanager.google.com",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}