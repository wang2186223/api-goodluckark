{
  "redirects": [
    {
      "source": "/",
      "destination": "https://admanager.google.com",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/(.*)",
      "destination": "https://admanager.google.com",
      "permanent": true,
      "statusCode": 301
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}